{
  "title": "My Dynamic Form",
  "customCSS": "",
  "inputs": [
    {
      "type": "label",
      "name": "demoText",
      "sx": { "width": "100%" },
      "text": "This is demo text that can be placed anywhere in teh form. Please fill out the fields below."
    },
    {
      "type": "text",
      "label": "First Name",
      "name": "fname",
      "validation": {
        "required": true
      },
      "sx": { "width": "50%" }
    },
    {
      "type": "text",
      "label": "Middle Name",
      "name": "mname",
      "validation": {
        "required": true
      }
    },
    {
      "type": "text",
      "label": "Last Name",
      "name": "lname",
      "validation": {
        "required": true
      }
    },
    {
      "type": "break"
    },
    {
      "type": "label",
      "name": "genderLabel",
      "text": "Gender",
      "sx": { "mt": 1, "mr": 2, "fontWeight": "bold" }
    },
    {
      "type": "radio",
      "name": "gender",
      "options": ["Male", "Female"],
      "validation": {
        "required": true
      }
    },
    {
      "type": "break"
    },
    {
      "type": "select",
      "label": "Favorite Color",
      "name": "color",
      "options": ["Red", "Green", "Blue"],
      "validation": {
        "required": true
      }
    },
    {
      "type": "checkbox",
      "label": "Agree to Terms",
      "name": "terms",
      "sx": { "width": "100%" }
    }
  ]
}
